{
  "expo": {
    "name": "moneycenter",
    "slug": "moneycenter",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.footalent.moneycenter"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.footalent.moneycenter"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font"
        // {
        //   "fonts": ["assets/fonts/Urbanist-Regular.ttf","assets/fonts/Urbanist-Bold.ttf","assets/fonts/Urbanist-ExtraBold.ttf"]
        // }
      ]
    ]
  }
}
